	Style: Test Style
	
	Title: Akada - Tests
	
	Division { ID: content }
		Image { ID: background; Source: res/background.jpg }
	
	Link { Reference: ../Calculus/Calculus.html } 
	
		Division { Class: test; ID: calculus }
			Image: Class { thumb } Source: res/calc.jpg
