Define: Test Style

	On: content
		
		margin: 0
		padding: 0

		top: 0%
		left: 0%

		height: 100vh
		
		position: fixed
		background: black

	On: background

		width: 100%

	test
		
		left: 10%

		width: 80%
		height: 30%

		position: absolute
		background: white

		border-radius: 25px
	
	thumb
		
		top: 10%
		left: 5%

		width: 30%
		height: 80%

		position: absolute

		border-radius: 25px
	
	name
		
		top: 7%
		left: 55%
		font-size: 4vw
		
		color: white
		text-shadow: 4px 4px 8px black

		position: absolute

	On: calculus: top: 10%
	On: biology: top: 50%
	On: compsci: top: 90%
	On: history: top: 130%
	On: english: top: 170%