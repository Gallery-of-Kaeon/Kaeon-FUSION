Use: Standard

i: 1
limit: Input: "Enter a number: "

Scope
	
	Scope
		
		Break: Not: And
			Equal: Modulus { i, 3 }, 0
			Equal: Modulus { i, 5 }, 0

		Log Line: fizz buzz
	
	Else { Break: Not: Equal: Modulus { i, 3 }, 0 }
		Log Line: fizz
	
	Else { Break: Not: Equal: Modulus { i, 5 }, 0 }
		Log Line: buzz
	
	Else
		Log Line: i
	
	i: Add: i, 1
	
	Loop: Less: i, limit